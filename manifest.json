{
    "manifest_version": 2,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ 	
								"jquery-1.8.3.min.js",
								"synccit.user.js"
 								
							],
        "matches":          [   "http://*.reddit.com/*",
								"http://reddit.com/*"
                            ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "description":  "synccit chrome extension",
    "name":         "synccit",
    "version":      "1",
	"permissions":	[	"<all_urls>"
						
		]
}